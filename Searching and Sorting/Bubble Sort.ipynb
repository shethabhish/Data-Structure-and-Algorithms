    "def bubble_sort(arr):\n",
    "    \n",
    "    for n in range(len(arr)-1, 0, -1):\n",
    "        for k in range(n):\n",
    "            if arr[k] > arr[k+1]:\n",
    "                temp = arr[k]\n",
    "                arr[k] = arr[k+1]\n",
    "                arr[k+1] = temp"
   ]
